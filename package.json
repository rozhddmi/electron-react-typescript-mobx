{
  "version": "1.0.0",
  "name": "electron-react-typescript-mobx",
  "author": "Don Alvarez <don@acceleratedpictures.com>",
  "description": "Boilerplate for electron + react + typescript + mobx apps",
  "keywords": [
    "electron",
    "react",
    "typescript",
    "mobx"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/yoDon/Electron-React-Typescript-MobX.git"
  },
  "license": "MIT",
  "scripts": {
    "start": "npm run start-pack && concurrently \"npm run start-dev\" \"npm run start-electron\"",
    "start-pack": "webpack --target web --config webpack.config.js && webpack --config webpack.config.js",
    "start-dev": "cross-env NODE_ENV=development webpack-dev-server",
    "start-electron": "tsc _main/index.ts && cross-env NODE_ENV=development electron _main/index",
    "build": "npm run dist && npm run build-all",
    "dist": "cross-env NODE_ENV=production webpack --config webpack.production.config.js",
    "build-all": "build -wl"
  },
  "devDependencies": {
    "@types/classnames": "^2.2.3",
    "@types/es6-collections": "^0.5.31",
    "@types/jquery": "^3.2.16",
    "@types/node": "^8.0.51",
    "@types/react": "^16.0.22",
    "@types/react-dom": "^16.0.3",
    "@types/react-router-dom": "^4.2.1",
    "awesome-typescript-loader": "^3.3.0",
    "concurrently": "^3.5.1",
    "cross-env": "^5.1.3",
    "css-loader": "^0.28.7",
    "electron": "^1.7.10",
    "electron-builder": "^19.51.0",
    "file-loader": "^1.1.5",
    "html-webpack-plugin": "^2.30.1",
    "node-sass": "^4.6.1",
    "npm-run-all": "^4.1.2",
    "react-hot-loader": "^3.1.3",
    "sass-loader": "^6.0.6",
    "source-map-loader": "^0.2.3",
    "style-loader": "^0.19.0",
    "tslint": "^5.9.1",
    "tslint-config-standard": "^7.0.0",
    "tslint-loader": "^3.5.3",
    "tslint-react": "^3.4.0",
    "typescript": "^2.6.1",
    "typings-for-css-modules-loader": "^1.7.0",
    "webpack": "^2.2.1",
    "webpack-dev-middleware": "^1.12.2",
    "webpack-dev-server": "^2.9.5",
    "webpack-hot-middleware": "^2.21.0"
  },
  "dependencies": {
    "asar": "^0.14.0",
    "classnames": "^2.2.5",
    "is-electron-renderer": "^2.0.0",
    "jquery": "^3.2.1",
    "minimist": "^1.2.0",
    "mobx": "^3.1.5",
    "mobx-react": "^4.1.3",
    "react": "^16.1.1",
    "react-dom": "^16.1.1",
    "react-electron-web-view": "^2.0.1",
    "react-hot-loader": "^3.1.3",
    "react-router": "^4.0.0",
    "react-router-dom": "^4.0.0"
  }
}
